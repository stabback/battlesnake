{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
      "trailing-comma": [true, {"multiline": "never", "singleline": "never"}],
      "no-console": false,
      "semicolon": false,
      "arrow-parens": [true, "ban-single-arg-parens"],
      "indent": [true, "spaces", 4]
  },
  "rulesDirectory": []
}