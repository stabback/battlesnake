<%- include('partials/header', {title: 'Logs | List'}); %>
<h1>List of games played</h1>

<% if(games.length === 0) { %>
  No games played yet!
<% } else { %>
    <ul>
    <% games.forEach(function(game) { %>
      <li><a href='logs/<%=game.id%>'><%= game.id %></a></li>
    <% }); %>
  </ul>
<% } %>


<%- include('partials/footer'); %>