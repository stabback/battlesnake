{
  "name": "stabback-battlesnake",
  "version": "1.0.0",
  "description": "A collection of my battlesnakes",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "build:watch": "npm run clean && tsc -w",
    "serve": "node dist/server.js",
    "serve:watch": "nodemon dist/server.js",
    "dev": "concurrently 'npm run build:watch' 'npm run serve:watch' -n build,serve -c magenta,green",
    "heroku": "npm run build && git push heroku master",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stabback/battlesnake"
  },
  "author": "Josh Stabback",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BattlesnakeOfficial/starter-snake-node/issues"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.9",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3"
  },
  "nodemonConfig": {
    "delay": "250"
  }
}
